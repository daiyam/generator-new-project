{
	"name": "@daiyam/generator-new-project",
	"description": "A generator to boilerplate my projects with my artifact configs",
	"version": "0.1.0",
	"author": {
		"name": "Baptiste Augrain",
		"email": "daiyam@zokugun.org"
	},
	"license": "MIT",
	"homepage": "https://github.com/daiyam/generator-new-project",
	"repository": {
		"type": "git",
		"url": "https://github.com/daiyam/generator-new-project.git"
	},
	"bugs": {
		"url": "https://github.com/daiyam/generator-new-project/issues"
	},
	"main": "generators/index.js",
	"scripts": {
		"commit": "cz",
		"compile": "tsc -p src",
		"lint": "xo",
		"prepare": "husky install; fixpack || true",
		"prepublishOnly": "npm run compile",
		"release": "release-it",
		"test": "tsc -p test && mocha",
		"test:dev": "mocha",
		"test:watch": "tsc-watch -p test --onSuccess 'mocha'",
		"watch": "tsc -watch -p src",
		"watch:test": "tsc-watch -p test"
	},
	"dependencies": {
		"yeoman-generator": "^5.4.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.1.0",
		"@commitlint/config-conventional": "^13.1.0",
		"@types/mocha": "^8.2.2",
		"@types/node": "^14.17.16",
		"@types/yeoman-assert": "^3.1.1",
		"@types/yeoman-generator": "^5.2.7",
		"@types/yeoman-test": "^4.0.3",
		"chai": "^4.3.4",
		"commitizen": "^4.2.4",
		"eslint-plugin-chai-friendly": "^0.7.2",
		"fixpack": "^4.0.0",
		"globby": "^11.0.4",
		"husky": "^7.0.1",
		"lint-staged": "^11.1.1",
		"mocha": "^8.3.2",
		"release-it": "^14.11.3",
		"source-map-support": "^0.5.20",
		"tsc-watch": "^4.4.0",
		"typescript": "^4.2.4",
		"xo": "^0.43.0",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^3.6.0",
		"yeoman-test": "^6.2.0"
	},
	"keywords": [
		"artifact",
		"boilerplate",
		"boilerplates",
		"codegeneration",
		"codemod",
		"codemods",
		"configuration",
		"configurations",
		"generate",
		"generator",
		"project-generator",
		"project-starter",
		"project-template",
		"scaffold",
		"yeoman-generator"
	]
}
